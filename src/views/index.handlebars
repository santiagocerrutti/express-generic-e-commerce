<div>
  {{#if products}}
  {{#each products}}
  <div>
    <div class="card" style="width: 18rem;">
      <img class="card-img-top" src={{this.thumbnails}} alt="Card image cap">
      <div class="card-body">
        <h5 class="card-title">{{this.title}}</h5>
        <p class="card-text">{{this.description}}</p>
        <a href="#" class="btn btn-outline-primary">$ {{this.price}}</a>
      </div>
    </div>
  </div>
  {{/each }}
  {{/if}}
</div>